{
  "name": "@my-task-app/source",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "format": "prettier --write .",
    "lint": "nx run-many --target=lint --all",
    "test": "nx run-many --target=test --all",
    "prepare": "husky install",
    "prisma:generate": "prisma generate --schema=apps/backend/prisma/schema.prisma",
    "prisma:migrate": "dotenv -e apps/backend/.env -- prisma migrate dev --schema=apps/backend/prisma/schema.prisma",
    "reset:testdb": "dotenv -e apps/backend/.env.test -- prisma migrate reset --force --schema=apps/backend/prisma/schema.prisma --skip-seed",
    "test:integration": "npm run reset:testdb && cross-env NODE_ENV=test jest --config jest.integration.config.ts"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@nx/esbuild": "21.1.3",
    "@nx/eslint-plugin": "^21.1.3",
    "@nx/jest": "21.1.3",
    "@nx/js": "21.1.3",
    "@nx/node": "^21.1.3",
    "@nx/react": "20.8.2",
    "@nx/vite": "^21.1.3",
    "@nx/web": "21.1.3",
    "@nx/workspace": "21.1.3",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.6.0",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.1.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^3.0.5",
    "@vitest/ui": "^3.0.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "esbuild": "^0.19.2",
    "eslint": "^9.28.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.1",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "5.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jiti": "2.4.2",
    "jsdom": "~22.1.0",
    "jsonc-eslint-parser": "^2.1.0",
    "nx": "21.1.3",
    "prettier": "^3.5.3",
    "pretty-quick": "^4.2.2",
    "prisma": "^6.9.0",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.33.1",
    "vite": "^6.0.0",
    "vitest": "^3.0.0"
  },
